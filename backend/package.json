{
  "name": "cheapship-backend",
  "version": "4.0.0",
  "description": "Backend API for Cheapship Analytics Platform with AI Insights",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "prisma:reset": "prisma migrate reset",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "build": "npm run prisma:generate",
    "deploy": "npm run build && npm start",
    "analytics:test": "node scripts/test-analytics.js",
    "health": "curl http://localhost:4000/api/health",
    "docs": "node scripts/generate-docs.js"
  },
  "keywords": [
    "cheapship",
    "delivery",
    "api",
    "analytics",
    "ai",
    "predictions",
    "mobile",
    "real-time",
    "insights",
    "performance",
    "tracking",
    "dashboard",
    "financial",
    "metrics"
  ],
  "author": "Cheapship Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheapship/backend"
  },
  "bugs": {
    "url": "https://github.com/cheapship/backend/issues"
  },
  "homepage": "https://cheapship.fr",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "prisma": "^6.14.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "stripe": "^18.4.0",
    "uuid": "^11.1.0",
    "compression": "^1.8.1",
    "express-winston": "^4.2.0",
    "winston": "^3.17.0",
    "joi": "^18.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "csv-parser": "^3.2.0",
    "json2csv": "^6.1.0",
    "qrcode": "^1.5.4",
    "pdf-lib": "^1.17.1",
    "express-prometheus-middleware": "^1.2.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "axios": "^1.11.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "eslint": "^9.33.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-jest": "^29.0.1",
    "prettier": "^3.6.2",
    "@types/jest": "^30.0.0",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "artillery": "^2.0.24",
    "clinic": "^13.0.0"
  },
  "peerDependencies": {
    "socket.io-client": "^4.8.1"
  },
  "optionalDependencies": {
    "redis": "^5.8.1",
    "ioredis": "^5.7.0",
    "elasticsearch": "^16.7.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/config/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,json",
    "ignore": [
      "src/**/*.test.js",
      "node_modules/**",
      "coverage/**",
      "uploads/**"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "config": {
    "analytics": {
      "enabled": true,
      "tracking": true,
      "predictions": true,
      "mobile": true
    },
    "features": {
      "alerts": true,
      "socket_io": true,
      "file_uploads": true,
      "rate_limiting": true,
      "compression": true,
      "monitoring": true
    },
    "performance": {
      "cache_enabled": true,
      "cluster_mode": false,
      "max_memory": "512MB"
    }
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/cheapship"
  },
  "contributors": [
    {
      "name": "Cheapship Development Team",
      "email": "dev@cheapship.fr",
      "url": "https://cheapship.fr/team"
    }
  ],
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}